"is_food":false,"disable_amount":true,"display":"zest of 2 large oranges, ~5 tsp","title":null,"original_text":null,"reference_id":"a265b350-fa54-41fa-b679-89ddc5229c29"},{"quantity":1.0,"unit":null,"food":null,"note":"⅛ tsp salt","is_food":false,"disable_amount":true,"display":"⅛ tsp salt","title":null,"original_text":null,"reference_id":"b9915446-b803-4bf0-b87b-c8f24c2aa074"},{"quantity":1.0,"unit":null,"food":null,"note":"½ tsp vanilla extract","is_food":false,"disable_amount":true,"display":"½ tsp vanilla extract","title":null,"original_text":null,"reference_id":"c3e4c13d-574b-4428-a46b-b3e216109b0a"},{"quantity":1.0,"unit":null,"food":null,"note":"¼ cup semi sweet mini chocolate chips","is_food":false,"disable_amount":true,"display":"¼ cup semi sweet mini chocolate chips","title":null,"original_text":null,"reference_id":"67f60b9a-5994-44c4-9155-cf6382a7242f"}],"recipe_instructions":[{"id":"ea7ce9b0-ed3f-49b0-9951-aae218c70c4c","title":"","summary":"","text":"Combine flour and salt in small bowl.","ingredient_references":[]},{"id":"5073e326-0713-4131-8ae0-75ead1c35835","title":"","summary":"","text":"Gather orange zest and set aside.","ingredient_references":[]},{"id":"427b6109-7037-440a-bc4b-9744399122c0","title":"","summary":"","text":"Place sugar in mixer bowl. Add zest.","ingredient_references":[]},{"id":"3954617a-8e0f-44e6-b4f8-035777a818f8","title":"","summary":"","text":"Use back of spoon to mash zest and sugar together.","ingredient_references":[]},{"id":"30a8c6b9-c6d7-420a-b61a-d1c3f1655c4f","title":"","summary":"","text":"Continue to mash until the sugar is moist and fragrant.","ingredient_references":[]},{"id":"db7ab90b-aeff-48d4-ad9c-ac7dfa0872fa","title":"","summary":"","text":"Add butter and vanilla. Mix on low for 1 minute.","ingredient_references":[]},{"id":"7a658d65-3ef7-4323-b0f2-9cb54c3435cc","title":"","summary":"","text":"Add the flour with salt and mix until just combined.","ingredient_references":[]},{"id":"2ed2d30a-9cec-4a4a-a4d1-21f6f99ce444","title":"","summary":"","text":"Fold in mini chocolate chips.","ingredient_references":[]},{"id":"4ccbc90a-b3b6-4d5f-bfde-b61ed54d7c0c","title":"","summary":"","text":"Form dough into a flat disc and chill for 30 minutes.","ingredient_references":[]},{"id":"54cbd5b2-13d2-411d-b8c1-205efc0e6c6a","title":"","summary":"","text":"preheat oven to 350 degrees Fahrenheit.","ingredient_references":[]},{"id":"12df6803-186a-40c5-b15d-67fd175f97c3","title":"","summary":"","text":"Roll dough into until ½ inch thick.","ingredient_references":[]},{"id":"1103c853-725c-43e8-8266-8125e27126f1","title":"","summary":"","text":"Use 2 inch cutter to make cookies.","ingredient_references":[]},{"id":"32926766-8889-4de4-aae6-3cd16105c10e","title":"","summary":"","text":"Put cookies on a sheet pan and chill for another 10 minutes in the freezer(this reduces spreading while baking).","ingredient_references":[]},{"id":"95ff82bd-9213-424f-93c1-6bc3a3516146","title":"","summary":"","text":"Bake for 25-28 minutes or until bottom of cookies are golden brown.","ingredient_references":[]},{"id":"f8d2eda9-45dc-4868-b184-fbe4404caf39","title":"","summary":"","text":"Cool completely and serve.","ingredient_references":[]}],"nutrition":{"calories":null,"carbohydrate_content":null,"cholesterol_content":null,"fat_content":null,"fiber_content":null,"protein_content":null,"saturated_fat_content":null,"sodium_content":null,"sugar_content":null,"trans_fat_content":null,"unsaturated_fat_content":null},"settings":{"public":true,"show_nutrition":false,"show_assets":false,"landscape_view":false,"disable_comments":false,"disable_amount":true,"locked":false},"assets":[],"notes":[],"extras":{},"comments":[]}