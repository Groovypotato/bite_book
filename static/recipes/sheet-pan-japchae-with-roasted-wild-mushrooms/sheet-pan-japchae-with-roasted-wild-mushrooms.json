s_food":false,"disable_amount":true,"display":"Olive oil","title":"","original_text":null,"reference_id":"820083aa-edcd-41be-a6b8-1f74e0583833"},{"quantity":1.0,"unit":null,"food":null,"note":"Kosher salt, freshly ground black pepper","is_food":false,"disable_amount":true,"display":"Kosher salt, freshly ground black pepper","title":"","original_text":null,"reference_id":"cb3f4f97-f115-4196-8c91-0415e1c2886e"},{"quantity":1.0,"unit":null,"food":null,"note":"2 tablespoons plus 1 teaspoon soy sauce","is_food":false,"disable_amount":true,"display":"2 tablespoons plus 1 teaspoon soy sauce","title":"","original_text":null,"reference_id":"0f3ba4aa-e53b-4eec-b370-5252b42708d2"},{"quantity":1.0,"unit":null,"food":null,"note":"1 tablespoon toasted sesame oil","is_food":false,"disable_amount":true,"display":"1 tablespoon toasted sesame oil","title":"","original_text":null,"reference_id":"1fd30ea1-96ae-4f83-932e-2dda3708b5ff"},{"quantity":1.0,"unit":null,"food":null,"note":"1 tablespoon dark brown sugar","is_food":false,"disable_amount":true,"display":"1 tablespoon dark brown sugar","title":"","original_text":null,"reference_id":"aee895a0-bb43-4a99-b560-f44a863b3cb2"},{"quantity":1.0,"unit":null,"food":null,"note":"1 tablespoon maple syrup","is_food":false,"disable_amount":true,"display":"1 tablespoon maple syrup","title":"","original_text":null,"reference_id":"15e685c4-6aca-4782-890e-b0a15f15f3ce"},{"quantity":1.0,"unit":null,"food":null,"note":"Toasted sesame seeds, for serving","is_food":false,"disable_amount":true,"display":"Toasted sesame seeds, for serving","title":"","original_text":null,"reference_id":"5363c949-2b84-4953-9b64-6ece34720da9"}],"recipe_instructions":[{"id":"6d016e4d-21e7-4b8c-994e-02d20f3f653c","title":"Do some prep:","summary":"","text":"Cut or tear 1 pound wild mushrooms into bite sized (1”-2”) pieces.","ingredient_references":[]},{"id":"5cd0bbbf-19ce-4f79-90fa-bbf6fd9fd121","title":"","summary":"","text":"Cut 5 scallions crosswise into 5” pieces.","ingredient_references":[]},{"id":"8c3ff1f4-eab7-42ca-804f-ae0e00438d5a","title":"","summary":"","text":"Position a rack as close to the broiler element as possible and preheat the broiler.","ingredient_references":[]},{"id":"279d4c38-7a72-4d63-af06-a18c0d292442","title":"","summary":"","text":"Bring a large pot of water to a boil.","ingredient_references":[]},{"id":"ae07a8f2-6f20-4567-8a93-cbe8852ed151","title":"Roast the veg:","summary":"","text":"Arrange the mushrooms and scallions on a large rimmed baking sheet and drizzle generously with olive oil. Place under the broiler and roast until deeply caramelized and crispy, about 5 minutes, shaking the pan halfway through. This will greatly vary depending on the strength of your broiler. Watch them carefully.","ingredient_references":[]},{"id":"594339d2-0230-432a-b9e0-5c3cbcc149f0","title":"","summary":"","text":"Remove from the oven and season with salt and pepper.","ingredient_references":[]},{"id":"0f88e980-f7d5-416e-abd2-df1a46b5858c","title":"Make the sauce:","summary":"","text":"In a small bowl, whisk together 2 tablespoons plus 1 teaspoon soy sauce, 1 tablespoon toasted sesame oil, 1 tablespoon dark brown sugar, 1 tablespoon maple syrup, and 1 finely grated large clove of garlic.","ingredient_references":[]},{"id":"db472844-229c-4d97-80d9-758cac219694","title":"Cook the noodles and serve:","summary":"","text":"Cook the dangmyeon noodles according to package instructions.","ingredient_references":[]},{"id":"25c7afcc-76a2-4d68-bfe8-60f62fc84b08","title":"","summary":"","text":"When the noodles are done cooking, drain them and add them directly to the sheet pan with the roasted mushrooms.","ingredient_references":[]},{"id":"b9fe2122-1963-4a73-9171-61f0ecf1746e","title":"","summary":"","text":"Pour the dressing over the noodles and toss everything together with tongs.","ingredient_references":[]},{"id":"d8b58ff1-c1d2-4a0b-8fab-f682016273d1","title":"","summary":"","text":"Transfer the japchae to a large platter and sprinkle with sesame seeds.","ingredient_references":[]}],"nutrition":{"calories":null,"carbohydrate_content":null,"cholesterol_content":null,"fat_content":null,"fiber_content":null,"protein_content":null,"saturated_fat_content":null,"sodium_content":null,"sugar_content":null,"trans_fat_content":null,"unsaturated_fat_content":null},"settings":{"public":true,"show_nutrition":false,"show_assets":false,"landscape_view":false,"disable_comments":false,"disable_amount":true,"locked":false},"assets":[],"notes":[],"extras":{},"comments":[]}