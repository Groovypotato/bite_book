uantity":1.0,"unit":null,"food":null,"note":"2 tsp. baking powder","is_food":false,"disable_amount":true,"display":"2 tsp. baking powder","title":null,"original_text":null,"reference_id":"7adc40f6-1e00-4708-9eb4-4c5176693ffb"},{"quantity":1.0,"unit":null,"food":null,"note":"1 Tbsp natural vanilla extract","is_food":false,"disable_amount":true,"display":"1 Tbsp natural vanilla extract","title":null,"original_text":null,"reference_id":"3820fbb9-c7d2-4924-a518-5d6acc94359d"},{"quantity":1.0,"unit":null,"food":null,"note":"Olive oil spray or butter spray","is_food":false,"disable_amount":true,"display":"Olive oil spray or butter spray","title":null,"original_text":null,"reference_id":"7f4f64db-d59a-4d7e-b928-8d172214c3ba"},{"quantity":1.0,"unit":null,"food":null,"note":"Powdered sugar as desired.","is_food":false,"disable_amount":true,"display":"Powdered sugar as desired.","title":null,"original_text":null,"reference_id":"8ca616f9-c123-4762-b21d-3f75ef9c553c"},{"quantity":1.0,"unit":null,"food":null,"note":"Optional: If you want to add anise for more traditional flavoring, I would try your first batch using 1/8 tsp anise.","is_food":false,"disable_amount":true,"display":"Optional: If you want to add anise for more traditional flavoring, I would try your first batch using 1/8 tsp anise.","title":null,"original_text":null,"reference_id":"4b21d163-5f40-4629-9034-85e4f9f8e5e2"}],"recipe_instructions":[{"id":"468c2abe-bdee-4353-b16c-21ad296cef09","title":"","summary":"","text":"Sift or whisk together flour and baking powder and then set aside.","ingredient_references":[]},{"id":"a0a2b7b7-e8cb-44c7-81e9-9e98f0676547","title":"","summary":"","text":"In another bowl, beat eggs and sugar together for about 3 minutes until they're fluffy.","ingredient_references":[]},{"id":"b472a657-d62c-44ba-80ce-f830981291db","title":"","summary":"","text":"While beating, slowly drizzle in the cool, melted butter. Then add in the vanilla extract. (If you are using anise, add it now.)","ingredient_references":[]},{"id":"34523604-5a90-46e0-b9bf-4607592cf4ad","title":"","summary":"","text":"Add in the flour mix little by little and beat until well combined.","ingredient_references":[]},{"id":"211a661e-1054-4aa9-8952-368d59b62ca3","title":"","summary":"","text":"Heat the pizzelle maker and then spray it with olive oil or butter. Add about 3 tablespoons of the dough (about half an ice cream scoop) just behind the center of the pizzelle maker. It will spread as the iron comes down.","ingredient_references":[]},{"id":"a775c2e6-0428-4944-aedf-e74ddcf37b5f","title":"","summary":"","text":"Cook for 30 seconds.","ingredient_references":[]},{"id":"bf5ecad3-4ac9-4db2-9ede-13265315ead4","title":"","summary":"","text":"Use a metal spatula to remove the cookies and place directly on a cooling rack","ingredient_references":[]},{"id":"55833332-e25e-49e7-a35c-08af385d22db","title":"","summary":"","text":"Let them cool completely and then dust with powdered sugar.","ingredient_references":[]}],"nutrition":{"calories":null,"carbohydrate_content":null,"cholesterol_content":null,"fat_content":null,"fiber_content":null,"protein_content":null,"saturated_fat_content":null,"sodium_content":null,"sugar_content":null,"trans_fat_content":null,"unsaturated_fat_content":null},"settings":{"public":true,"show_nutrition":false,"show_assets":false,"landscape_view":false,"disable_comments":false,"disable_amount":true,"locked":false},"assets":[],"notes":[],"extras":{},"comments":[]}