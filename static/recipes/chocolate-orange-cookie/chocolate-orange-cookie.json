is_food":false,"disable_amount":true,"display":"2 large eggs","title":null,"original_text":null,"reference_id":"1e63540b-c4b1-4d28-9fb1-6cb381d65362"},{"quantity":1.0,"unit":null,"food":null,"note":"2 tsp natural vanilla extract","is_food":false,"disable_amount":true,"display":"2 tsp natural vanilla extract","title":null,"original_text":null,"reference_id":"6310d8ce-8b02-464f-b567-7b55214255bc"},{"quantity":1.0,"unit":null,"food":null,"note":"1 tsp natural orange bakery emulsion","is_food":false,"disable_amount":true,"display":"1 tsp natural orange bakery emulsion","title":null,"original_text":null,"reference_id":"752783b1-2363-44da-a676-d0e079ffc3c8"},{"quantity":1.0,"unit":null,"food":null,"note":"1 orange (zested)","is_food":false,"disable_amount":true,"display":"1 orange (zested)","title":null,"original_text":null,"reference_id":"c7ba1c57-7e0b-42ad-ba87-f0e35814e1da"},{"quantity":1.0,"unit":null,"food":null,"note":"4 cups all-purpose flour","is_food":false,"disable_amount":true,"display":"4 cups all-purpose flour","title":null,"original_text":null,"reference_id":"2ef309c9-c285-4ffc-a99c-85d0e8eecd6a"},{"quantity":1.0,"unit":null,"food":null,"note":"1 1/4 tsp baking soda","is_food":false,"disable_amount":true,"display":"1 1/4 tsp baking soda","title":null,"original_text":null,"reference_id":"43533b7d-abc5-4661-bfe0-9e01c1bd9974"},{"quantity":1.0,"unit":null,"food":null,"note":"1 1/2 tsp baking powder","is_food":false,"disable_amount":true,"display":"1 1/2 tsp baking powder","title":null,"original_text":null,"reference_id":"5d4a648f-5c47-43c5-b04b-605c5db5d15f"},{"quantity":1.0,"unit":null,"food":null,"note":"1 tsp coarse salt","is_food":false,"disable_amount":true,"display":"1 tsp coarse salt","title":null,"original_text":null,"reference_id":"c641ccf9-44c5-4ce2-954f-67e538e867a0"},{"quantity":1.0,"unit":null,"food":null,"note":"12 oz milk chocolate bars, chopped","is_food":false,"disable_amount":true,"display":"12 oz milk chocolate bars, chopped","title":null,"original_text":null,"reference_id":"8c8f2ade-08d6-4c8a-8390-94783501130f"},{"quantity":1.0,"unit":null,"food":null,"note":"flakey sea salt","is_food":false,"disable_amount":true,"display":"flakey sea salt","title":null,"original_text":null,"reference_id":"da5701fd-1f5c-4661-abdf-caa879fbe927"},{"quantity":1.0,"unit":null,"food":null,"note":"15 chocolate orange slices","is_food":false,"disable_amount":true,"display":"15 chocolate orange slices","title":null,"original_text":null,"reference_id":"f1fd914d-060a-4665-bbdc-9c9f0ae93fc1"}],"recipe_instructions":[{"id":"b1e531b0-a466-4b4d-85b9-67cc074abe70","title":"","summary":"","text":"Preheat your oven to 375°F (190°C). Line baking sheets with parchment paper.","ingredient_references":[]},{"id":"7228c2e1-d66a-467b-bd64-283e68f85162","title":"","summary":"","text":"Cream together softened butter, light brown sugar, and granulated sugar for a full 5 minutes.","ingredient_references":[]},{"id":"b934f636-3e54-4f9e-b322-4d27527ebc0a","title":"","summary":"","text":"Beat in eggs one at a time, then add vanilla extract, orange bakery emulsion and orange zest. Mix well.","ingredient_references":[]},{"id":"88e8d73a-926f-4814-b733-434c73848a45","title":"","summary":"","text":"In a separate bowl, whisk together all-purpose flour, baking soda, baking powder, and coarse salt.","ingredient_references":[]},{"id":"85365ac1-0677-49da-a3f8-4df34b375d77","title":"","summary":"","text":"Gradually add the dry ingredients to the wet ingredients, mixing until just combined.","ingredient_references":[]},{"id":"d64987f3-3662-4d03-a6b6-3223c4b212b9","title":"","summary":"","text":"Gently fold in the chopped milk chocolate bars until evenly distributed.","ingredient_references":[]},{"id":"6f8da177-7249-4f67-a831-dc940eb15b1d","title":"","summary":"","text":"Using a #12 scoop, scoop out portions of dough onto prepared baking sheets, leaving enough space between cookies.Top with flakey sea salt and more chopped chocolate, if desired.","ingredient_references":[]},{"id":"25243cb4-d101-4612-99a8-2a602610df37","title":"","summary":"","text":"Bake for about 12-15 minutes or until the cookie has spread slightly and is golden brown.","ingredient_references":[]},{"id":"2f33658d-93f1-4041-b526-cfe5f22aaa73","title":"","summary":"","text":"As soon as the cookies come out of the oven, gently press a chocolate orange slice into the top of each cookie.","ingredient_references":[]},{"id":"a72ece12-7a01-4106-9f63-4fcbc238629c","title":"","summary":"","text":"Allow the cookies to cool on the baking sheets for 25 minutes before transferring them to wire racks to cool completely.","ingredient_references":[]}],"nutrition":{"calories":"403","carbohydrate_content":"61","cholesterol_content":"65","fat_content":"16","fiber_content":"1","protein_content":"4","saturated_fat_content":"10","sodium_content":"384","sugar_content":"35","trans_fat_content":"1","unsaturated_fat_content":"5"},"settings":{"public":true,"show_nutrition":false,"show_assets":false,"landscape_view":false,"disable_comments":false,"disable_amount":true,"locked":false},"assets":[],"notes":[],"extras":{},"comments":[]}