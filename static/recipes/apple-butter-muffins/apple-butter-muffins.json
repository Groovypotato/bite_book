od":null,"note":"200 grams sugar (about 1 cup)","is_food":false,"disable_amount":true,"display":"200 grams sugar (about 1 cup)","title":null,"original_text":null,"reference_id":"2f6bc3a1-e673-4e54-ac27-5f7e71c7651d"},{"quantity":1.0,"unit":null,"food":null,"note":"210 grams flour (about 1 3/4 cups when measured correctly by scooping and leveling)","is_food":false,"disable_amount":true,"display":"210 grams flour (about 1 3/4 cups when measured correctly by scooping and leveling)","title":null,"original_text":null,"reference_id":"df6c6bc9-dc51-4aaa-b2bc-5ffb03faf475"},{"quantity":1.0,"unit":null,"food":null,"note":"1 teaspoon kosher salt (6 grams)","is_food":false,"disable_amount":true,"display":"1 teaspoon kosher salt (6 grams)","title":null,"original_text":null,"reference_id":"0e9d05ae-2c1a-45ce-8a25-2b8b593b51ab"},{"quantity":1.0,"unit":null,"food":null,"note":"1 teaspoon baking soda (5 grams)","is_food":false,"disable_amount":true,"display":"1 teaspoon baking soda (5 grams)","title":null,"original_text":null,"reference_id":"a99bf111-0f03-4905-bc17-3d1c998dd60b"},{"quantity":1.0,"unit":null,"food":null,"note":"1 teaspoon baking powder (5 grams)","is_food":false,"disable_amount":true,"display":"1 teaspoon baking powder (5 grams)","title":null,"original_text":null,"reference_id":"22704549-6bdd-44cc-b5ca-32d9d366f009"},{"quantity":1.0,"unit":null,"food":null,"note":"2 Tablespoons ground cinnamon (10 grams)","is_food":false,"disable_amount":true,"display":"2 Tablespoons ground cinnamon (10 grams)","title":null,"original_text":null,"reference_id":"95120fc7-09d3-4951-b243-832201590c3c"},{"quantity":1.0,"unit":null,"food":null,"note":"1 teaspoon ground ginger","is_food":false,"disable_amount":true,"display":"1 teaspoon ground ginger","title":null,"original_text":null,"reference_id":"0402eb31-2c09-422d-82ea-86f5edb685bf"},{"quantity":1.0,"unit":null,"food":null,"note":"1/2 teaspoon ground nutmeg","is_food":false,"disable_amount":true,"display":"1/2 teaspoon ground nutmeg","title":null,"original_text":null,"reference_id":"b7d2c470-a4c8-49f0-bdcf-e8fa4e265f51"},{"quantity":1.0,"unit":null,"food":null,"note":"1/2 teaspoon ground cloves","is_food":false,"disable_amount":true,"display":"1/2 teaspoon ground cloves","title":null,"original_text":null,"reference_id":"b9c06264-f6ca-4bbf-ab32-d803e3720f92"},{"quantity":1.0,"unit":null,"food":null,"note":"200 grams apple butter (about 3/4 cup)","is_food":false,"disable_amount":true,"display":"200 grams apple butter (about 3/4 cup)","title":null,"original_text":null,"reference_id":"a2e3e94f-75aa-4907-bbf8-f2c2f6ecb91f"},{"quantity":1.0,"unit":null,"food":null,"note":"1/2 cup melted butter (113g)","is_food":false,"disable_amount":true,"display":"1/2 cup melted butter (113g)","title":null,"original_text":null,"reference_id":"ab1f7b80-c155-4bb6-a4e9-834487f7cb6f"},{"quantity":1.0,"unit":null,"food":null,"note":"2 eggs","is_food":false,"disable_amount":true,"display":"2 eggs","title":null,"original_text":null,"reference_id":"cb9e30fc-0dd0-4b3d-9a13-f961ce04ee8d"},{"quantity":1.0,"unit":null,"food":null,"note":"1/2 teaspoon vanilla extract","is_food":false,"disable_amount":true,"display":"1/2 teaspoon vanilla extract","title":null,"original_text":null,"reference_id":"5140e50c-f6e1-4ebb-94eb-f6900ac47651"}],"recipe_instructions":[{"id":"e330488e-c88d-4bad-9356-277a73e88908","title":"","summary":"","text":"Preheat the oven & prepare your muffin tins:","ingredient_references":[]},{"id":"32f8cec5-2987-4193-bc36-0887d40c74ce","title":"","summary":"","text":"Heat oven to 425° Fahrenheit. NOTE: you will reduce this temperature part way through baking.","ingredient_references":[]},{"id":"924edfd2-08d7-43c1-bc2c-f728443dc427","title":"","summary":"","text":"Line muffin tins with paper liners or silicone liners.","ingredient_references":[]},{"id":"8dd68d26-2745-4c4e-a4ff-3b5e4ce08f7c","title":"","summary":"","text":"Mix the crumb topping:","ingredient_references":[]},{"id":"e53e9f20-d908-49e8-adf5-6bfe28340670","title":"","summary":"","text":"Mix the brown sugar, cinnamon, flour, and softened butter in a small bowl to form a sandy, lumpy mixture. Set aside while making the muffin batter.","ingredient_references":[]},{"id":"8a9ad634-d8fd-4d5b-b7b5-e14b578a74a3","title":"","summary":"","text":"MIX THE MUFFIN BATTER:","ingredient_references":[]},{"id":"546b5733-6d0c-4960-9af6-27ee4f2138f1","title":"","summary":"","text":"In a large bowl, stir together sugar, flour, salt, baking soda, baking powder, cinnamon, ginger, nutmeg, and cloves.","ingredient_references":[]},{"id":"9cc41dc7-9afd-4373-85aa-868d2435009f","title":"","summary":"","text":"In a small bowl or large measuring cup, whisk together apple butter, melted butter, eggs, and vanilla.","ingredient_references":[]},{"id":"16305fa4-23d2-4b6d-85bc-240a56104822","title":"","summary":"","text":"Pour wet ingredients into dry ingredients and mix until no lumps remain.","ingredient_references":[]},{"id":"63312988-fa1b-4443-83b7-800fe59abefb","title":"","summary":"","text":"Spoon batter into your prepared muffin tins. Fill tins about 2/3 full. (We like to use a cookie scoop to make sure all muffins are the same size).","ingredient_references":[]},{"id":"784d0c10-9994-48c6-8dd1-b105520cb388","title":"","summary":"","text":"Top each muffin with 1 or 2 teaspoons of the crumb topping.","ingredient_references":[]},{"id":"5409ab0e-ed90-4143-82a6-2964cbd27f1b","title":"","summary":"","text":"Put muffins into the oven. Bake for 5 minutes at 425° F, then reduce the temperature to 375° F and bake an additional 10-15 minutes, until a cake tester inserted into the center of a muffin comes out clean.","ingredient_references":[]},{"id":"4a6e2038-a991-49be-9e23-cdb764dc1dac","title":"","summary":"","text":"Transfer muffins to a wire cooling rack. Serve warm or let cool completely before storing.","ingredient_references":[]}],"nutrition":{"calories":"276","carbohydrate_content":"42.8","cholesterol_content":"56.4","fat_content":"10.7","fiber_content":"1.9","protein_content":"3.4","saturated_fat_content":"6.3","sodium_content":"316.6","sugar_content":"24.1","trans_fat_content":"0","unsaturated_fat_content":null},"settings":{"public":true,"show_nutrition":false,"show_assets":false,"landscape_view":false,"disable_comments":false,"disable_amount":true,"locked":false},"assets":[],"notes":[],"extras":{},"comments":[]}