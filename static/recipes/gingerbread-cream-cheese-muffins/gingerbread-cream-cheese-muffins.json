e_id":"673f83d0-52de-42a4-a8fd-a79e8d2533eb"},{"quantity":1.0,"unit":null,"food":null,"note":"1/4 teaspoon salt","is_food":false,"disable_amount":true,"display":"1/4 teaspoon salt","title":"","original_text":null,"reference_id":"ec665245-a0da-4b90-a25d-00a5363a608d"},{"quantity":1.0,"unit":null,"food":null,"note":"1/4 cup canola oil (1/4 cup butter, softened, may be substituted)","is_food":false,"disable_amount":true,"display":"1/4 cup canola oil (1/4 cup butter, softened, may be substituted)","title":"","original_text":null,"reference_id":"b9f9c505-c694-4bb0-ac32-ad132dcbdd85"},{"quantity":1.0,"unit":null,"food":null,"note":"1/2 cup light brown sugar","is_food":false,"disable_amount":true,"display":"1/2 cup light brown sugar","title":"","original_text":null,"reference_id":"3e0b6b9b-f86b-47d4-ad0b-9775d3981795"},{"quantity":1.0,"unit":null,"food":null,"note":"3 tablespoons molasses","is_food":false,"disable_amount":true,"display":"3 tablespoons molasses","title":"","original_text":null,"reference_id":"7eff3cb9-8f63-40a3-8d00-8f6135eef6cf"},{"quantity":1.0,"unit":null,"food":null,"note":"3 tablespoons honey","is_food":false,"disable_amount":true,"display":"3 tablespoons honey","title":"","original_text":null,"reference_id":"c454dcad-7dae-4c20-8666-a7f500634ada"},{"quantity":1.0,"unit":null,"food":null,"note":"2 eggs","is_food":false,"disable_amount":true,"display":"2 eggs","title":"","original_text":null,"reference_id":"96151898-4ca5-4506-9c89-ef1c76301a1b"},{"quantity":1.0,"unit":null,"food":null,"note":"1 cup buttermilk","is_food":false,"disable_amount":true,"display":"1 cup buttermilk","title":"","original_text":null,"reference_id":"d7e34b22-9e70-4585-a536-b00bb3953635"},{"quantity":1.0,"unit":null,"food":null,"note":"Reserved cream cheese filling","is_food":false,"disable_amount":true,"display":"Reserved cream cheese filling","title":"","original_text":null,"reference_id":"8f053749-f815-4ea2-b83f-8799813afb80"}],"recipe_instructions":[{"id":"fedf4f3f-ca17-45e1-8cce-588bc423418c","title":"FOR CREAM CHEESE FILLING:","summary":"","text":"Combine sugar and butter in bowl of electric mixer fitted with paddle attachment and beat on low speed until there are no lumps. Add cream cheese and mix on medium speed for 3 minutes, until smooth.","ingredient_references":[]},{"id":"3408e1f5-e442-4dee-bcc4-426b1f132a2f","title":"","summary":"","text":"Reduce speed to low. Add egg and lemon juice and mix until smooth.","ingredient_references":[]},{"id":"c19bd260-0ef3-4950-9764-94db88819aca","title":"","summary":"","text":"Refrigerate at least 2 hours, up to 3 days, until ready to use.","ingredient_references":[]},{"id":"6378c47f-3e8e-4c34-a859-c1ed97a28c21","title":"FOR GINGERBREAD-CREAM CHEESE MUFFINS:","summary":"","text":"Preheat oven to 325Â°F. Prepare a muffin pan with 12 liners and set aside.","ingredient_references":[]},{"id":"d9be2fcc-53db-4a21-9fc7-a3916a2e038e","title":"","summary":"","text":"Sift flour, ginger, cinnamon, nutmeg, cloves, baking powder, baking soda, and salt into medium bowl and set aside.","ingredient_references":[]},{"id":"1c096df9-5b78-4e5c-8e4c-371b69227c18","title":"","summary":"","text":"Combine oil or butter, brown sugar, molasses, honey, and egg in bowl of electric mixer fitted with whisk attachment. Mix on medium speed until combined. Reduce speed to low and add buttermilk until incorporated.","ingredient_references":[]},{"id":"aa6cd8f6-5fcc-449a-b2dc-8135a5c797ed","title":"","summary":"","text":"Add flour mixture and mix on low speed until combined. Scrape sides of mixer and mix an additional 30 seconds. Batter will be slightly lumpy.","ingredient_references":[]},{"id":"f8698c54-d112-4748-88a3-e5144606108b","title":"","summary":"","text":"Scoop batter into prepared muffin pan, filling each cup 3/4 full.","ingredient_references":[]},{"id":"080ae90b-795b-4df9-83f2-073a22489984","title":"","summary":"","text":"Place reserved cream cheese filling in piping bag fitted with large round tip. Insert piping tip into the center of each muffin, piping about 1 tablespoon into the center, then pipe a swirl around the top of the muffin.","ingredient_references":[]},{"id":"88e76699-ab6c-4900-bb23-d21f5db98142","title":"","summary":"","text":"Bake for 20-25 minutes, until top of muffin is firm when touched.","ingredient_references":[]},{"id":"4ff0f577-7279-49f0-ba72-4cdcce49bb1d","title":"","summary":"","text":"Cool before serving.","ingredient_references":[]}],"nutrition":{"calories":null,"carbohydrate_content":null,"cholesterol_content":null,"fat_content":null,"fiber_content":null,"protein_content":null,"saturated_fat_content":null,"sodium_content":null,"sugar_content":null,"trans_fat_content":null,"unsaturated_fat_content":null},"settings":{"public":true,"show_nutrition":false,"show_assets":false,"landscape_view":false,"disable_comments":false,"disable_amount":true,"locked":false},"assets":[],"notes":[],"extras":{},"comments":[]}